                        <real>2.00</real>
                    </coefficient>
                </element>
            <rhs>
                <real>5.0</real>
            </rhs>
            <operator>leq</operator>
        </constraint>
        <constraint id="con2">
                <element>
                    <alternativeID>B</alternativeID>
                    <coefficient>
                        <real>1.00</real>
                    </coefficient>
                </element>
                <element>
                    <alternativeID>C</alternativeID>
                    <coefficient>
                        <real>2.00</real>
                    </coefficient>
                </element>
                <element>
                    <alternativeID>D</alternativeID>
                    <coefficient>
                        <real>1.00</real>
                    </coefficient>
                </element>
            <rhs>
                <real>3.0</real>
            </rhs>
            <operator>eq</operator>
        </constraint>

    </alternativesLinearConstraints>


</xmcda:XMCDA>