<?xml version="1.0" encoding="iso-8859-1"?>
<program_description xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="../../docs/w3-ws/specificationData/description.xsd">

    <program provider="PUT" name="PrometheeV" displayName="PrometheeV" version="0.2.0" />
    <documentation>
        <description>Promethee V - choose alternatives using linear constraints.</description>
        <url>https://github.com/dohko93/mcdm/tree/master/PrometheeV</url>
    </documentation>
    <parameters>

        <input id="input1" name="alternatives" displayName="alternatives" isoptional="0">
            <documentation>
                <description>List of alternatives.</description>
            </documentation>
            <xmcda tag="alternatives" />
        </input>
        <input id="input2" name="constraints" displayName="constraints" isoptional="0">
            <documentation>
                <description>Linear constraints.</description>
            </documentation>
            <xmcda tag="alternativesLinearConstraints" />
        </input>
        <input id="input3" name="flows" displayName="flows" isoptional="0">
            <documentation>
                <description>Net flows score for each alternative.</description>
            </documentation>
            <xmcda tag="alternativesValues" />
        </input>

        <output id="output1" name="result" displayName="chosenAlternatives">
            <documentation>
                <description>List of alternatives values where alternatives inside optimal set have value equal to 1, rest are outside optimal set.</description>
            </documentation>
            <xmcda tag="alternativesValues" />
        </output>

        <output id="output2" name="messages" displayName="messages">
            <documentation>
                <description>Messages or errors generated by this module.</description>
            </documentation>
            <xmcda tag="methodMessages"/>
       </output>

    </parameters>
</program_description>
